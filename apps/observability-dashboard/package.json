{
  "name": "observability-dashboard",
  "version": "1.0.0",
  "description": "Real-time Multi-Agent Observability Dashboard built with Vue 3, TypeScript, and WebSockets",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.4.8",
    "pinia": "^2.3.0",
    "chart.js": "^4.4.6",
    "vue-chartjs": "^5.3.2",
    "primevue": "^4.5.3",
    "primeicons": "^7.1.0",
    "@fortawesome/fontawesome-free": "^6.7.1",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "vite": "^6.0.5",
    "vue-tsc": "^2.2.0",
    "typescript": "~5.7.0",
    "@types/node": "^22.10.5",
    "@types/lodash-es": "^4.17.12",
    "vitest": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^25.0.1",
    "eslint": "^9.15.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/eslint-config-prettier": "^10.1.0",
    "prettier": "^3.4.2",
    "sass": "^1.81.1"
  },
  "claude_agent": {
    "agent_id": "observability-ui-agent",
    "project_type": "frontend_dashboard",
    "tech_stack": ["vue3", "typescript", "pinia", "websockets", "chartjs"],
    "primary_tasks": [
      "real_time_monitoring",
      "event_visualization",
      "agent_coordination_display",
      "performance_analytics",
      "responsive_design"
    ]
  }
}