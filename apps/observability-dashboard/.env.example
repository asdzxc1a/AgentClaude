# Multi-Agent Observability Dashboard Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:4000
VITE_WS_URL=ws://localhost:4000

# Application Configuration
VITE_APP_TITLE=Multi-Agent Observability Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Real-time monitoring dashboard for Claude Code multi-agent coordination

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_WEBSOCKET=false
VITE_MOCK_DATA=false

# Feature Flags
VITE_ENABLE_THEMES=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Performance Configuration
VITE_MAX_EVENTS_MEMORY=1000
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=5
VITE_WEBSOCKET_RECONNECT_DELAY=1000
VITE_CHART_UPDATE_INTERVAL=1000

# Analytics (Optional)
VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_ID=

# Error Reporting (Optional)
VITE_ERROR_REPORTING_ENABLED=false
VITE_SENTRY_DSN=